<h1> Hey <%= @user.name %> </h1>
<h2><%= @user.room.name %> Class </h2>
<%= button_to 'New Day', new_forms_path %>
<% @children.each do |child| %>
  <%= link_to child.name, child_path(child.id) %>
  <% info = child.infos.last %>
  <%= link_to "Edit info", edit_user_child_info(@user.id, child.id, info.id) %>
<% end %>
